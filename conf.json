{
  "use": "puppeteer",
  "headless": true,
  "seeds": [
    "https://www.mirena-us.com/"
  ],
  "warc": {
    "naming": "url",
    "append": true
  },
  "connect": {
    "launch": true,
    "host": "localhost",
    "port": 9222
  },
  "crawlControl": {
    "globalWait": 3000,
    "inflightIdle": 1000,
    "numInflight": 2,
    "navWait": 4000,
    "mode": "page-same-domain",
    "depth": 2
  }
}
